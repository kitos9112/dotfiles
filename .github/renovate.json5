{
  "extends": [
    ":enableRenovate",
    "config:recommended",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":separatePatchReleases",
    "docker:enableMajor",
    ":enablePreCommit",
    "github>kitos9112/dotfiles//.github/renovate/regexManagers.json5",
  ],
  "timezone": "Europe/London",
  "dependencyDashboard": true,
  "dependencyDashboardTitle": "Renovate Dashboard",
  "labels": [
    "renovatebot"
  ],
  "commitMessageSuffix": "[ci-skip]",
  "gitAuthor": "henry-pa-bot <166536+henry-bot[bot]@users.noreply.github.com>",
  "suppressNotifications": [
    "prIgnoreNotification"
  ],
  "ignoreTests": true,
  "rebaseWhen": "conflicted",
  "assignees": [
    "@kitos9112"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["nodejs"],
      "minimumReleaseAge": "10 days",
    },
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ],
  "asdf": {
    "fileMatch": [
      "(^|/)\\.tool-versions$",
      "(^|/)dot_tool-versions.tmpl$",
    ]
  }
}